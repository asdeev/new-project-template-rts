    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@popperjs/core": "^2.4.0",
    "bootstrap": "^4.5.0",
    "overmind": "^23.1.0",
    "overmind-react": "^24.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-popper": "^2.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "reactstrap": "^8.4.1"
  }
